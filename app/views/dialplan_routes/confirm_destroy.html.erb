<%= simple_form_for( @dialplan_route, :html => { :method => :delete } ) do |f| %>
	<%= f.error_messages %>
	<h2><%= t(:confirm_destroy_record, :model_name => t("activerecord.models.dialplan_route"))%></h2>
	<%= render 'show' %>
	<%= f.button :submit, t(:delete) if can?( :destroy, @dialplan_route ) %>
	<%= link_to t(:cancel), dialplan_routes_path if can?( :index, DialplanRoute ) %>
<% end %>