<h1>Editing extension</h1>

<% if ! @sip_account.nil? %>
  <%= render 'form_sip_account' %>
<% elsif ! @conference.nil? %>
  <%= render 'form_conference' %>
<% elsif ! @call_queue.nil? %>
  <%= render 'form_call_queue' %>
<% else %>
    <%= render 'form' %>
<% end %>

<%= link_to 'Show', @extension %> |
<%= link_to 'Back', extensions_path %>
