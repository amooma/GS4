<%= simple_form_for( @call_queue, :html => { :method => :delete } ) do |f| %>
	<%= f.error_messages %>
	<h2><%= t(:confirm_destroy_record, :model_name => t("activerecord.models.call_queue"))%></h2>
	<%= render 'show' %>
	<%= f.button :submit, t(:delete) if can?( :destroy, @call_queue ) %>
	<%= link_to t(:cancel), call_queues_path if can?( :index, CallQueue ) %>
<% end %>