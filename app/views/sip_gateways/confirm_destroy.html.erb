<%= simple_form_for( @sip_gateway, :html => { :method => :delete } ) do |f| %>
	<%= f.error_messages %>
	<h2><%= t(:confirm_destroy_record, :model_name => t("activerecord.models.sip_gateway"))%></h2>
	<%= render 'show' %>
	<%= f.button :submit, t(:delete) if can?( :destroy, @sip_gateway ) %>
	<%= link_to t(:cancel), sip_gateways_path if can?( :index, SipGateway ) %>
<% end %>