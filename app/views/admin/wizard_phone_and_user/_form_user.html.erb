
	<%= f_sip_account.input :user, :label => false, :hint => false do %>
		<%# f.simple_fields_for @phone.sip_accounts.first.user do |f_user| %>
		<% f_sip_account.simple_fields_for @user do |f_user| %>
			<%= f_user.input :gn, :hint => t('simple_form.hints.user.gn') %>
			<%= f_user.input :sn, :hint => t('simple_form.hints.user.sn') %>
			<%= f_user.input :username, :hint => t('simple_form.hints.user.username') %>
			<%= f_user.input :email, :hint => t('simple_form.hints.user.email') %>
			<%= f_user.input :password, :hint => t('simple_form.hints.user.password') %>
			<%= f_user.input :password_confirmation, :hint => t('simple_form.hints.user.password_confirmation') %>
			<%= f_user.input :role, :as => :hidden %>				
		<% end %>
	<% end %>
