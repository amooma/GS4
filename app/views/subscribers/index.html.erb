<h1><%= t(:listing_subscribers) %></h1>

<table>
  <tr>
    <th><%= t(:username) %></th>
    <th><%= t(:domain) %></th>
    <th><%= t(:password) %></th>
    <th><%= t(:email_address) %></th>
    <th><%= t(:ha1) %></th>
    <th><%= t(:ha1b) %></th>
    <th><%= t(:rpid) %></th>
    <th></th>
  </tr>

<% @subscribers.each do |subscriber| %>
  <tr>
    <td><%= subscriber.username %></td>
    <td><%= subscriber.domain %></td>
    <td><%= subscriber.password %></td>
    <td><%= subscriber.email_address %></td>
    <td><%= subscriber.ha1 %></td>
    <td><%= subscriber.ha1b %></td>
    <td><%= subscriber.rpid %></td>
    <td><%= link_to t(:show), subscriber %></td>
  </tr>
<% end %>
</table>

<br />
